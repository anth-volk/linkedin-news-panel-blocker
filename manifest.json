{
  "content_scripts": [
    {
      "css": [
        "linkedin-news-panel-blocker.css"
      ],
      "matches": [
        "*://*.linkedin.com/*"
      ]
    }
  ],
  "description": "Block the right-side LinkedIn News Panel to focus more on what's important: your network.",
  "icons": {
    "16": "img/ghost-16.png",
    "128": "img/ghost-128.png",
    "512": "img/ghost-512.png"
  },
  "manifest_version": 2,
  "name": "LinkedIn News Panel Blocker",
  "version": "0.0.1"
}
